<head>
  <%= render "shared/welcome_link" %>
  <h1>Viewing Party Application</h1>
</head>

<body>
  <%= button_to "Create New User", new_user_path, class: "button", method: :post %>
  <h3>Existing Users</h3>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to user.name, user_path(user) %></li>
    <% end %>  
  </ul>
</body>

